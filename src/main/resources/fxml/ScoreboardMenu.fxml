<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.text.Text?>
<Pane prefWidth="400" prefHeight="400" xmlns:fx="http://javafx.com/fxml"
      fx:controller="YuGiOh.graphicView.ScoreboardMenuView" styleClass="main">
    <VBox fx:id="mainBox" alignment="CENTER" spacing="13">
        <Label text="All Time High Scores:" StackPane.alignment="TOP_CENTER" styleClass="title"/>
        <Button alignment="CENTER" defaultButton="true" onMouseClicked="#exit"
                mnemonicParsing="false" text="Back" styleClass="button"/>
    </VBox>

    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="30.0"/>
    </padding>
    <stylesheets>
        <URL value="@../css/MainStyle.css"/>
    </stylesheets>
</Pane>