<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<Pane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" styleClass="background"
      fx:controller="YuGiOh.graphicView.DeckMenuView" prefHeight="640.0" prefWidth="600.0">
    <ListView fx:id="listView" maxHeight="500"/>

    <HBox fx:id="buttonHBox" alignment="CENTER" spacing="7">
        <Button alignment="CENTER" defaultButton="true" onMouseClicked="#exit" opacity="0"
                mnemonicParsing="false" text="Buy" styleClass="button" fx:id="buyButton"/>
        <Button alignment="CENTER" defaultButton="true" onMouseClicked="#exit"
                mnemonicParsing="false" text="Back" styleClass="button"/>
    </HBox>

    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="30.0"/>
    </padding>
    <stylesheets>
        <URL value="@../css/MainStyle.css"/>
    </stylesheets>
</Pane>
